<p id="notice"><%= notice %></p>

<h1>Listing Test Attachments</h1>

<table>
  <thead>
    <tr>
      <th>Attachment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_attachments.each do |test_attachment| %>
      <tr>
        <td><%= test_attachment.attachment %></td>
        <td><%= link_to 'Show', test_attachment %></td>
        <td><%= link_to 'Edit', edit_test_attachment_path(test_attachment) %></td>
        <td><%= link_to 'Destroy', test_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test attachment', new_test_attachment_path %>
